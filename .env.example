# Server
PORT=4336
NODE_ENV=development
CORS_ORIGIN=http://localhost:3000

# JWT (mesmo secret do Backend principal)
JWT_SECRET=your-secret-key-change-in-production
JWT_EXPIRE=7d

# MongoDB (mesmo do Backend - para User.scrapingCredits)
MONGODB_URI=mongodb://user:pass@host:27017/clerky?authSource=admin

# PostgreSQL (mesmo do Backend - buscas e resultados)
POSTGRES_URI=postgres://user:pass@host:5433/clerkydb?sslmode=disable

# Google Places API (New)
GOOGLE_PLACES_API_KEY=your-google-places-api-key

# Socket do Backend (para atualizar saldo em tempo real no frontend)
BACKEND_SOCKET_URL=http://localhost:4331
